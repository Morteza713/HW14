<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/profileFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.movieapp.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home"
        />
    <fragment
        android:id="@+id/comingSoonFragment"
        android:name="com.example.movieapp.ComingSoonFragment"
        android:label="ComingSoonFragment"
        tools:layout="@layout/fragment_coming_soon"
        />
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.example.movieapp.FavoriteFragment"
        android:label="FavoriteFragment"
        tools:layout="@layout/fragment_favorite"/>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.movieapp.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile"/>
</navigation>